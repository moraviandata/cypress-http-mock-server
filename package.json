{
  "name": "example-nextjs",
  "version": "1.0.0",
  "types": "lib/index.d.ts",
  "description": "Cypress-http-mock-server is a library for mocking HTTP responses to requests made from SSR. This is useful for testing frameworks such as Next.js and Nuxt.js.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "start": "node ./lib/index.js"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moraviandata/cypress-http-mock-server.git"
  },
  "keywords": [
    "http",
    "server",
    "cypress",
    "mock",
    "ssr",
    "testing"
  ],
  "author": "birdthedeveloper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moraviandata/cypress-http-mock-server/issues"
  },
  "homepage": "https://github.com/moraviandata/cypress-http-mock-server#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/supertest": "^2.0.12",
    "axios": "^1.5.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/http-server": "^0.12.1",
    "express": "^4.18.2",
    "ts-command-line-args": "^2.5.1",
    "yargs": "13.2"
  },
  "jest": {
    "testTimeout": 5000
  },
  "bin": {
    "cypress-http-mock-server": "./lib/index.js"
  }
}
